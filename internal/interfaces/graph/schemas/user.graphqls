# GraphQL schema example
#
# https://gqlgen.com/getting-started/
scalar DateTime

type User {
  id: ID!
  email: String!
  updatedAt: DateTime!
  createdAt: DateTime!
}

type Query {
  users: [User!]!
}

input SignUpInput {
  email: String!
  password: String!
}

type Mutation {
  signUp(input: SignUpInput!): User!
}
